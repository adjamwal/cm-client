#!/bin/bash

PREFIX="/opt/cisco/secureclient"
CM_DIR="${PREFIX}/cloudmanagement"
UNINST="${CM_DIR}/bin/cm_uninstall.sh"

#TODO : Uninstall and move config files based on versions - on upgrade, disallow downgrades based on versions

#for now just uninstalling previous installations
${UNINST}

